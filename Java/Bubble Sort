class BubbleSort
{
	public static void main(String[] args)
	{
		int [] a= { 4,11,98,111,200};
		int tmp;
		for(int i=0; i<a.length; i++)
		{
			int flg = 0;
			for(int j=0; j<a.length-1-i; j++)
			{
				if(a[j] > a[j+1])
				{
					tmp = a[j];
					a[j] = a[j+1];
					a[j+1] = tmp;
					flg = 1;
				}
			}
			if(flg == 0)
				break;
		}
		for(int i=0; i<a.length; i++)
		{
			System.out.print(a[i]+" ");
		}
	}
}
