#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int n;
	    cin >> n;
	    if(n < 4)
	        cout << "YES\n";
	        
	    else if(n % 4 == 0)
	        cout << "NO\n";
	        
	    else if(n > 4 && n % 4 != 0)
	        cout << "YES\n";
	}
	return 0;
}
