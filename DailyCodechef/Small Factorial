#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int n;
	    cin >> n;
	    int fact ;
	    if(n == 0 || n == 1)
	        fact = 1;
	        
	    else
	    {
	        fact = 1;
	        while(n > 0)
	        {
	            fact = fact * n;
	            n--;
	        }
	    }
	    cout << fact << "\n";
	}
	return 0;
}
