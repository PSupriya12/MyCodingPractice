#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t ;
	cin >> t ;
	while(t--)
	{
	    int arr[10] ;
	    for(int i=0; i<10; i++)
	    {
	        cin >> arr[i] ;
	    }
	    int cnt1 = 0, cnt2 = 0 ;
	    for(int i=0; i<10; i=i+2)
	    {
	        if(arr[i] == 1)
	            cnt1++;
	    }
	    for(int i=1; i<10; i=i+2)
	    {
	        if(arr[i] == 1)
	            cnt2++;
	    }
	    if(cnt1 > cnt2)
	        cout << "1\n";
	        
	    else if(cnt1 == cnt2)
	        cout << "0\n";
	        
	    else if(cnt2 > cnt1)
	        cout << "2\n";
	}
	return 0;
}
