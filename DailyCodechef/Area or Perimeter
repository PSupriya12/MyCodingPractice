#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int l,b;
	cin >> l;
	cin >> b;
	int p,a;
	p = 2*(l + b);
	a = l * b;
	if(p > a)
	    cout << "Peri" << endl << p;
	    
	 else if(a > p)
	    cout << "Area" << endl << a;
	    
	 else
	    cout << "Eq" << endl << a;
	return 0;
}
