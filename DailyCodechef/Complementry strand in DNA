#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int n;
	    cin >> n;
	    char str[n];
	    for(int i=0; i<n; i++)
	    {
	        cin >> str[i];
	    }
	    for(int i=0; i<n; i++)
	    {
	        if(str[i] == 'A')
	        {
	            str[i] = 'T';
	            cout << str[i];
	        }
	            
	        else if(str[i] == 'T')
	        {
	            str[i] = 'A';
	            cout << str[i];
	        }
	            
	        else if(str[i] == 'C')
	        {
	            str[i] = 'G';
	            cout << str[i];
	        }
	            
	        else if(str[i] == 'G')
	        {
	            str[i] = 'C';
	            cout << str[i];
	        }
	    }
	    cout << endl;
	}
	return 0;
}
