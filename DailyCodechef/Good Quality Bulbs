#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int n,x ;
	    cin >> n >> x;
	    int arr[n-1];
	    for(int i=0; i<n-1; i++)
	    {
	        cin >> arr[i];
	    }
	    int sum = 0;
	    for(int i=0; i<n-1; i++)
	    {
	        sum = sum + arr[i];
	    }
	    int ans;
	    ans = (x * n) - sum;
	    if(ans < 0)
	        cout << "0\n";
	    else
	        cout << ans << endl;
	}
	return 0;
}
