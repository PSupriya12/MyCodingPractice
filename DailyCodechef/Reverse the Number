#include <iostream>
using namespace std;

int main() 
{
    int t ;
    cin>>t;
    while(t--)
    {
        int N ;
        cin>>N ;
        long long int rev=0;
        while(N>0)
        {
            rev=(rev*10)+N%10;
            N=N/10;
        }
        cout<<rev<<"\n";
    }
	return 0;
}

