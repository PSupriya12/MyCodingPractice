#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int xa, xb, xc;
	    cin >> xa >> xb >> xc;
	    if(xa > 50)
	        cout << "A\n";
	        
	    else if(xb > 50)
	        cout << "B\n";
	        
	    else if(xc > 50)
	        cout << "C\n";
	        
	    else
	        cout << "NOTA\n";
	}
	return 0;
}
