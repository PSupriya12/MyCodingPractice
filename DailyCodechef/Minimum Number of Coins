#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t, x, cnt;
	cin>>t;
	while(t--)
	{
	    cin>>x;
	    if(x%10 == 0)
	        cnt = x / 10 ;
	    else if(x%5 == 0)
	    {
	        cnt = (x/10) + ((x%10) / 5) ;
	    }
	    else 
	        cnt = -1;
	    cout<<cnt<<endl;
	}
	return 0;
}
