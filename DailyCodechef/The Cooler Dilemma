#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--)
	{
	    int x, y;
	    cin >> x >> y;
	    int mon;
	    if(x >= y)
	        mon = 0;
	        
	    else      
	    {
	        mon = (y - 1) / x;
	    }
	    
	    cout << mon << endl;
	}
	return 0;
}
