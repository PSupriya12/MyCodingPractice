#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int T,pa,pb,qa,qb,tp,tq;
	cin>>T;
	while(T--)
	{
	    cin>>pa>>pb>>qa>>qb;
	    if(pa>=pb)
	        tp = pa ;
	        
	    else
	        tp = pb ;
	   
	    if(qa>=qb)
	        tq = qa ;
	    
	    else
	        tq = qb ;
	        
	    if(tp<tq)
	    {
	        cout<<"P"<<endl;
	    }
	    else if(tp>tq)
	    {
	        cout<<"Q"<<endl;
	    }
	    else if(tp==tq)
	    {
	        cout<<"TIE\n";
	    }
	}
	return 0;
}
