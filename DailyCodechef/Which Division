#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int r;
	    cin >> r;
	    if(r >= 2000)
	        cout << "1\n";
	        
	    else if(r >= 1600 && r < 2000)
	        cout << "2\n";
	        
	    else if(r < 1600)
	        cout << "3\n";
	}
	return 0;
}
