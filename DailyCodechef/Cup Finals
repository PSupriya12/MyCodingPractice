#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t, x, y, d;
	cin>>t;
	while(t--)
	{
	    cin>>x>>y>>d;
	    if(x>=y)
	    {
	        if((x-y)<=d)
	            cout<<"Yes\n";
	        else
	            cout<<"No\n";
	    }
	    else if(y>x)
	    {
	        if((y-x)<=d)
	            cout<<"Yes\n";
	        else
	            cout<<"No\n";
	    }
	   
	}
	return 0;
}
