#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int arr[7];
	    for(int i=0; i<7; i++)
	    {
	        cin >> arr[i];
	    }
	    int cnt = 0;
	    for(int i=0; i<7; i++)
	    {
	        if(arr[i] == 1)
	            cnt++;
	    }
	    if(cnt > (7-cnt))
	        cout << "Yes\n";
	        
	    else
	        cout << "No\n";
	}
	return 0;
}
