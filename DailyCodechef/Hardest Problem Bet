#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int sa, sb, sc;
	    cin >> sa >> sb >> sc;
	    if(sc < sb && sc < sa)
	        cout << "Alice\n";
	        
	    else if(sb < sa && sb < sc)
	        cout << "Bob\n";
	        
	    else
	        cout << "Draw\n";
	}
	return 0;
}
