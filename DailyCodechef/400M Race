#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int x, y, z;
	    cin >> x >> y >> z;
	    if(x < y && x < z)
	        cout << "Alice\n";
	        
	    else if(y < x && y < z)
	        cout << "Bob\n";
	        
	    else if(z < x && z < y)
	        cout << "Charlie\n";
	}
	return 0;
}
