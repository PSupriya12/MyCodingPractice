#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int n, k;
	    cin >> n >> k;
	    int arr[n];
	    for(int i=0; i<n; i++)
	    {
	        cin >> arr[i];
	    }
	    int cnt = 0;
	    for(int i=0; i<n; i++)
	    {
	        if(arr[i] > k)
	            cnt++ ;
	    }
	    cout << cnt << "\n" ;
	}
	return 0;
}
