#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t, a, b, c;
	cin>>t;
	while(t--)
	{
	    cin>>a>>b>>c;
	    if((a>b && a<c) || (a>c && a<b))
	        cout<<a<<"\n";
	        
	    else if((b>a && b<c) || (b>c && b<a))
	        cout<<b<<"\n";
	        
	    else if((c>a && c<b) || (c>b && c<a))
	        cout<<c<<"\n";
	}
	return 0;
}
