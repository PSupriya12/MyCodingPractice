#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int n;
	    cin >> n;
	    char b[n];
	    for(int i=0; i<n; i++)
	    {
	        cin >> b[i];
	    }
	    int cnt = 0;
	    for(int i=0; i<n; i++)
	    {
	        if(b[i] == '1')
	            cnt++;
	    }
	    if((cnt + (120 - n)) >= 90)
	        cout << "YES\n";
	        
	    else
	        cout << "NO\n";
	}
	return 0;
}
