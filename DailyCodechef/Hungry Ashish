#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int x,y,z;
	    cin >> x >> y >> z;
	    if(x >= y)
	        cout << "PIZZA\n";
	        
	    else if(x < y && x >= z)
	        cout << "BURGER\n";
	        
	    else if(x < y && x < z)
	        cout << "NOTHING\n";
	}
	return 0;
}
