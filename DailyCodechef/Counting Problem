#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int n;
	    cin >> n;
	    int arr[n];
	    for(int i=0 ; i<n ; i++)
	    {
	        cin >> arr[i];
	    }
	    int odd=0 , even=0 ;
	    for(int i=0 ; i<n ; i++)
	    {
	        if( arr[i] % 2 == 0)
	        {
	            even++;
	        }
	        else 
	        {
	            odd++;
	        }
	    }
	    if ( odd == 0 || odd % 2 != 0 )
	    {
	        cout << "NO" << endl;
	    }
	    else 
	    {
	        cout << "YES" << endl;
	    }
	}
	return 0;
}
