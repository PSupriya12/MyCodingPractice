#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int n;
	    cin >> n;
	    int s[n];
	    for(int i=0; i<n; i++)
	    {
	        cin >> s[i];
	    }
	    int d[n];
	    for(int i=0; i<n; i++)
	    {
	        cin >> d[i];
	    }
	    int cnt = 0;
	    for(int i=0; i<n; i++)
	    {
	        if(s[i] == d[i])
	           cnt++;
	    }
	    cout << cnt << "\n";
	}
	return 0;
}
