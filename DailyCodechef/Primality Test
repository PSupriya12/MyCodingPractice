#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--)
	{
	    int n;
	    cin >> n;
	    int i = 2;
	    for(i; i<n; i++)
	    {
	        if(n % i == 0)
	            break;
	    }
	    if(i == n)
	        cout << "yes\n";
	   
	    else
	        cout << "no\n";
	}
	return 0;
}
